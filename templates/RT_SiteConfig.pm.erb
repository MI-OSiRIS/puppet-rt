# This file is managed by puppet
use utf8;
<% @mergedsiteconfig.each do |key,value| -%>
  <%- if key[0..0] == '%' -%>
Set(<%= key %>,
<% value.sort.each do |k,v| -%>
  '<%= k %>' => '<%= v %>',
<%- end -%>
);
<%- elsif key[0..0] == '@' -%>
Set(<%= key %>,
<% value.sort.each do |v| -%>
  '<%= v %>',
<% end -%>
);
<%- elsif key[0..0] == '$' and value.is_a? Hash -%>
Set(<%= key %>, {
<% value.sort.each do |k,v| -%>
  '<%= k %>' => '<%= v %>',
<%- end -%>
});
<%- else -%>
Set(<%= key %>, '<%= value %>');
<%- end -%>
<% end -%>
1;
