Set(<%= @title %>,
<% @value.sort.each do |v| -%>
  '<%= v %>',
<% end -%>
);
<%- elsif @key[0..0] == '$' and value.is_a? Hash -%>
Set(<%= @title %>, {
<% @value.sort.each do |k,v| -%>
  '<%= k %>' => '<%= v %>',
<%- end -%>
});
<%- else -%>
Set(<%= @title %>, '<%= @value %>');
<%- end -%>
